package org.example.softwaredevelopmentproject.reviews;

import jakarta.persistence.*;
import lombok.*;

import java.time.LocalDate;
@Getter
@Setter
@AllArgsConstructor
@NoArgsConstructor
@Entity(name = "reviews")
public class Review {

    @Id
    @GeneratedValue(strategy = GenerationType.IDENTITY)
    private long id;

    @Column(nullable = false, length = 100)
    private long user;

    @Column(nullable = false, length = 100)
    private String role;

    private int rating;

    @Column(nullable = false, length = 100)
    private String review;
    

}
